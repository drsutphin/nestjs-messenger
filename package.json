{
  "name": "nestjs-messenger",
  "version": "1.2.1",
  "description": "Fully-featured messaging module for NestJS, providing email and SMS sending capabilities",
  "author": "Simon Emms <simon@simonemms.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -Rf ./dist",
    "build": "tsc -p ./tsconfig.build.json",
    "postbuild": "copyfiles CONTRIBUTING.md LICENSE package.json package-lock.json README.md dist",
    "dev": "nodemon -x 'npm start' -e js,ts,json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .ts,.js ./src",
    "start": "ts-node -r reflect-metadata ./example/src/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "email",
    "mjml",
    "nodemailer",
    "sms"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/mrsimonemms/nestjs-messenger"
  },
  "bugs": "https://gitlab.com/mrsimonemms/nestjs-messenger/-/issues",
  "peerDependencies": {
    "@nestjs/common": "*",
    "handlebars": "*",
    "pug": "*"
  },
  "optionalDependencies": {
    "pug": "*"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "lodash.set": "^4.3.2",
    "mjml": "^4.6.2",
    "nodemailer": "^6.4.6"
  },
  "devDependencies": {
    "@nestjs/common": "^7.0.7",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.2.1",
    "@types/lodash.set": "^4.3.6",
    "@types/mjml": "^4.0.4",
    "@types/node": "^13.11.1",
    "@types/nodemailer": "^6.4.0",
    "@types/pug": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "handlebars": "^4.7.6",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "devDependencies2": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/testing": "6.1.1",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
